#include<iostream>
using namespace std;
int main(){
    int x,x',v,v';
    cin>>x>>v>>x'>>v';
    if(x>x'){
        if(v>=v')
        cout<<"NO";
        else 
        cout<<"YES"; 
    }
    else if(x'>x){
        if(v'>=v)
        cout<<"NO";
        else 
        cout<<"YES";
    }
    else{
        if(v==v')
        cout<<"YES";
        else 
        cout<<"NO";
    }
}
