#include<iostream>
#include<string>
#include<string.h>
using namespace std;
int main(){
    string s,s';
    int k,n,n';
    cin>>s>>s'>>k;
    int a=s.size();
    int b=s'.size();
    if(s1==s2){
        int p=a+b;
        if(k>=p)
        cout<<"Yes";
        else 
        cout<<"No";
    }
    else if(b>a){
        if(k==(b-a))
        cout<<"Yes";
        else 
        cout<<"No";
    }
    else{
    for(int i=0,j=0;(i<a),(j<b);i++,j++){
            if(s1[i]==s2[j])
            n=i;
            n'=j;
            break;
        }
        a=a-n;
        b=b-n';
        int r=k-a+b;
        if(r>=0)
        cout<<"Yes";
        else 
        cout<<"No";
    }

}
